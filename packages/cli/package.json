{
  "name": "@react-bratus/cli",
  "version": "2.0.3",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stephanboersma/react-bratus.git"
  },
  "description": "React Bratus cli",
  "main": "./bin/index.js",
  "bin": {
    "react-bratus": "./bin/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build",
    "build": "tsc -p .",
    "local": "prototype",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "author": "Stephan Boersma",
  "license": "MIT",
  "dependencies": {
    "@react-bratus/parser": "^2.0.3",
    "@react-bratus/server": "^2.0.3",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^7.0.0",
    "figlet": "^1.5.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "gitHead": "6de840ebd664b9cae7defc39031816cb106a62ad"
