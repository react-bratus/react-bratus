    "clear": "^0.1.0",
    "commander": "^7.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "figlet": "^1.5.0",
    "fs": "^0.0.1-security",
    "glob": "^7.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.11.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^14.14.25",
