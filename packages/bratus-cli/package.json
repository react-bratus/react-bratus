{
  "name": "@react-bratus/cli",
  "version": "3.2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-bratus/react-bratus.git"
  },
  "description": "React Bratus cli",
  "main": "./bin/index.js",
  "bin": {
    "bratus": "./bin/index.js"
  },
  "scripts": {
